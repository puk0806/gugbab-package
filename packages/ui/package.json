{
  "name": "@gugbab-package/ui",
  "version": "0.0.0",
  "description": "gugbab-package ui",
  "author": "gugbab",
  "keywords": [
    "gugbab"
  ],
  "license": "MIT",
  "sideEffects": false,
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.umd.cjs",
  "files": [
    "dist/**",
    "public/styles/abstracts",
    "public/styles/components.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsc && tsup",
    "typecheck": "tsc -b tsconfig.json",
    "biome": "biome format --write",
    "ui-compile": "sh ./scripts/ui-compile.sh",
    "postbuild": "sh ./scripts/postbuild.sh"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "dependencies": {
    "@gugbab-package/icons": "workspace:*",
    "@gugbab-package/utils": "workspace:*",
    "radix-ui": "^1.1.3"
  },
  "devDependencies": {
    "@gugbab-package/biome-config": "workspace:*",
    "@gugbab-package/tsconfig": "workspace:*",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "sass": "^1.80.6",
    "tsup": "^8.0.2"
  }
}
