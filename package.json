{
  "name": "gugbab-package",
  "version": "0.0.0",
  "description": "gugbab-package",
  "author": "gugbab",
  "keywords": ["gugbab"],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dev": "turbo run dev",
    "dev:ui": "pnpm --filter @gugbab-package/ui dev",
    "build": "turbo run build",
    "build:ui": "pnpm --filter @gugbab-package/ui build",
    "build:utils": "pnpm --filter @gugbab-package/utils build",
    "build:icons": "pnpm --filter @gugbab-package/icons build",
    "biome": "turbo run biome",
    "biome:ui": "pnpm --filter @gugbab-package/ui biome",
    "biome:utils": "pnpm --filter @gugbab-package/utils biome",
    "biome:icons": "pnpm --filter @gugbab-package/icons biome",
    "typecheck": "turbo run typecheck",
    "typecheck:ui": "pnpm --filter @gugbab-package/ui typecheck",
    "typecheck:utils": "pnpm --filter @gugbab-package/utils typecheck",
    "clear-install": "bash scripts/clear-install/index.sh",
    "use-hook": "git config core.hooksPath .githooks && touch githooks.config.json"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "devDependencies": {
    "@gugbab-package/biome-config": "workspace:*",
    "@gugbab-package/tsconfig": "workspace:*",
    "@biomejs/biome": "^1.9.4",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  }
}
